{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "b",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "b",
      "kebab-case"
    ],
    "no-unused-expression": false,
    "no-input-rename": false,
    "no-unnecessary-initializer": false,
    "no-empty-interface": false,
    "deprecation": false
  }
}