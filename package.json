{
  "name": "bob-storybook",
  "version": "4.3.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run storybook --quiet",
    "start:app-test": "ng serve --project=app-test",
    "build": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build ui-framework",
    "test": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng test ui-framework",
    "lint": "ng lint ui-framework",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "schematics:watch": "cd ./bob-schematics/ && npm link && cd .. && npm link bob-schematics && npm run build --prefix ./bob-schematics -- --watch",
    "schematics:install": "npm run build --prefix ./bob-schematics && npm i ./bob-schematics --no-save",
    "storybook": "npm run docs:json && start-storybook -p 9001 -c .storybook --quiet",
    "build-storybook": "build-storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "npm_pack": "cd dist/ui-framework && npm pack",
    "package": "npm run build && npm run copy-style && npm run npm_pack",
    "copy-style": "cpx \"projects/ui-framework/src/lib/style/**/*\" \"dist/ui-framework/style/\"",
    "start-app-test": "ng serve --project=app-test",
    "postinstall": "ngcc"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@schematics/angular": "^11.2.14",
    "@types/highcharts": "^5.0.44",
    "ag-grid-angular": "^22.1.1",
    "ag-grid-community": "^22.1.1",
    "ag-grid-enterprise": "^22.1.1",
    "core-js": "^3.15.2",
    "date-fns": "^2.22.1",
    "froala-editor": "^4.0.3",
    "highcharts": "^7.2.2",
    "lodash": "^4.17.21",
    "ngx-color-picker": "^11.0.0",
    "rxjs": "^6.6.3",
    "tributejs": "^4.1.1",
    "tslib": "^2.3.0",
    "xss": "^1.0.9",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular-devkit/core": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@angular/language-service": "^12.1.1",
    "@babel/core": "^7.14.6",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-docs": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.3.2",
    "@storybook/angular": "^6.3.2",
    "@storybook/storybook-deployer": "^2.8.10",
    "@types/estree": "0.0.47",
    "@types/jasmine": "^3.7.8",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.15",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.0.6",
    "codelyzer": "^6.0.1",
    "cpx": "^1.5.0",
    "fsevents": "^2.1.3",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-mocks": "^10.0.0",
    "ng-packagr": "^12.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-mocks": "^3.0.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.10.1",
    "tslint": "~6.1.0",
    "typescript": "^4.1.6"
  },
  "publishConfig": {
    "registry": "https://hibob.jfrog.io/hibob/api/npm/bob-js-virtual/"
  },
  "storybook-deployer": {
    "commitMessage": "Deploy Storybook [skip ci]"
  }
}
