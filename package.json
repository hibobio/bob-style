{
  "name": "bob-storybook",
  "version": "4.2.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run storybook",
    "start:app-test": "ng serve --project=app-test",
    "build": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build ui-framework",
    "test": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng test ui-framework",
    "lint": "ng lint ui-framework",
    "schematics:watch": "cd ./bob-schematics/ && npm link && cd .. && npm link bob-schematics && npm run build --prefix ./bob-schematics -- --watch",
    "schematics:install": "npm run build --prefix ./bob-schematics && npm i ./bob-schematics --no-save",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "npm_pack": "cd dist/ui-framework && npm pack",
    "package": "npm run build && npm run copy-style && npm run npm_pack",
    "copy-style": "cpx \"projects/ui-framework/src/lib/style/**/*\" \"dist/ui-framework/style/\"",
    "start-app-test": "ng serve --project=app-test",
    "postinstall": "ngcc"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.0.2",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "^10.0.2",
    "@angular/compiler": "^10.0.2",
    "@angular/core": "^10.0.2",
    "@angular/forms": "^10.0.2",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "^10.0.2",
    "@angular/platform-browser-dynamic": "^10.0.2",
    "@angular/router": "^10.0.2",
    "@ngx-translate/core": "^12.1.2",
    "@schematics/angular": "^10.0.1",
    "@types/highcharts": "^5.0.44",
    "ag-grid-angular": "^22.1.1",
    "ag-grid-community": "^22.1.1",
    "ag-grid-enterprise": "^22.1.1",
    "core-js": "^3.6.4",
    "date-fns": "^2.9.0",
    "froala-editor": "^3.1.1",
    "highcharts": "^7.1.2",
    "lodash": "^4.17.15",
    "ngx-smooth-dnd": "^0.4.1",
    "rxjs": "^6.5.4",
    "tributejs": "^4.1.1",
    "tslib": "^2.0.0",
    "xss": "^1.0.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular-devkit/core": "^10.0.0",
    "@angular/cli": "^10.0.0",
    "@angular/compiler-cli": "^10.0.1",
    "@angular/language-service": "^10.0.1",
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/angular": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.3",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.39",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "fsevents": "^2.1.3",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
