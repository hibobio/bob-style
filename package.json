{
  "name": "bob-storybook",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run storybook",
    "start:app-test": "ng serve --project=app-test",
    "build": "ng build ui-framework",
    "test": "ng test ui-framework",
    "lint": "ng lint ui-framework",
    "schematics:watch": "cd ./bob-schematics/ && npm link && cd .. && npm link bob-schematics && npm run build --prefix ./bob-schematics -- --watch",
    "schematics:install": "npm run build --prefix ./bob-schematics && npm i ./bob-schematics",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "npm_pack": "cd dist/ui-framework && npm pack",
    "package": "npm run build && npm run copy-style && npm run npm_pack",
    "copy-style": "cpx \"projects/ui-framework/src/lib/style/**/*\" \"dist/ui-framework/style/\"",
    "start-app-test": "ng serve --project=app-test",
    "postinstall": "npm run schematics:install"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.2",
    "@angular/compiler": "~8.0.2",
    "@angular/core": "~8.0.2",
    "@angular/forms": "~8.0.2",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.0.2",
    "@angular/platform-browser-dynamic": "~8.0.2",
    "@angular/router": "~8.0.2",
    "@types/highcharts": "^5.0.44",
    "@types/quill": "^1.3.6",
    "ag-grid-angular": "^20.0.0",
    "ag-grid-community": "^20.0.0",
    "ag-grid-enterprise": "^20.0.0",
    "angular-froala-wysiwyg": "^3.0.6",
    "bob-schematics": "file:bob-schematics",
    "core-js": "^2.6.1",
    "date-fns": "^2.4.1",
    "hammerjs": "^2.0.8",
    "highcharts": "^7.1.2",
    "lodash": "^4.17.13",
    "ng-mocks": "^8.0.0",
    "quill": "^1.3.6",
    "rxjs": "~6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.3",
    "@angular-devkit/build-ng-packagr": "~0.800.3",
