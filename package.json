    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/router": "^9.0.1",
    "@types/highcharts": "^5.0.44",
    "ag-grid-angular": "^22.1.1",
    "ag-grid-community": "^22.1.1",
    "ag-grid-enterprise": "^22.1.1",
    "angular-froala-wysiwyg": "^3.1.0",
    "core-js": "^3.6.4",
    "date-fns": "^2.9.0",
    "hammerjs": "^2.0.8",
    "highcharts": "^7.1.2",
    "lodash": "^4.17.15",
    "ng-mocks": "^8.1.0",
    "ngx-smooth-dnd": "^0.4.1",
    "rxjs": "^6.5.4",
    "tributejs": "^4.1.1",
    "zone.js": "^0.10.2"
  },
