    "ng-packagr": "^10.0.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "protractor": "~7.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
